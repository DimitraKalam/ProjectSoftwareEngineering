<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="BorderPaneMainScreen" prefHeight="354.0" prefWidth="447.0" style="-fx-background-color: teal;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="MainScreen.MainScreenController">
   <left>
      <VBox prefHeight="200.0" prefWidth="114.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="AnnouncementsButton" mnemonicParsing="false" text="Announcements" />
            <Button fx:id="CalendarButton" mnemonicParsing="false" prefHeight="32.0" prefWidth="122.0" text="Calendar" />
            <Button fx:id="ToDoListButton" mnemonicParsing="false" prefHeight="32.0" prefWidth="114.0" text="To-do List" />
            <Button fx:id="ChatButton" mnemonicParsing="false" onAction="#goToChatScreen" prefHeight="32.0" prefWidth="120.0" text="Chat" />
         </children>
      </VBox>
   </left>
</BorderPane>
